<Window x:Class="Interface.ThankyouPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interface"
        mc:Ignorable="d"
        Title="ThankyouPopup" Height="300" Width="300">
    <Grid Margin="0,0,0,157">
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Thanks for being a client!" VerticalAlignment="Top" Margin="10,10,0,0"/>

    </Grid>
</Window>
